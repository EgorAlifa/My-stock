import java.util.Scanner;

class Egor{
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int a = sc.nextInt();
    int b = sc.nextInt();
    int c = sc.nextInt();
    if ((b*b-4*a*c)>0){
      System.out.print(2);
      }
     else if ((b*b-4*a*c)==0){
      System.out.print(1);
      }
     else if ((b*b-4*a*c)<0){
      System.out.print(0);
      }
     }
   }
