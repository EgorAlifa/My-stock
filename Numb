import java.until.Scanner;

class Egor{
  public static void(String[] args){
    Scanner sc = new Scanner(System.in);
      int a = sc.nextInt();
      int b = sc.nextInt();
      int c = sc.nextInt();
      if ((a>=c) && (a>=b)){
        System.out.print(a);
        }
       if ((b>=a) && (b>=c)){
        System.out.print(b);
       }
       if ((c>=a) && (c>=b)){
        System.out.print(c);
       }
     }
  }
