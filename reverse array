import java.util.Scanner;

class Egor{
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    int[] a = new int[n];
    int s = 0;
    for (int i = 0; i<n; i++){
      a[i]=sc.nextInt();
      s+=a[i];
     }
     System.out.println(s);
     for (int i =a.length-1;i>=0;i--){
        System.out.println(a[i]);
      }
   }
}
