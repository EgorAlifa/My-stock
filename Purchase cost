import java.util.Scanner;

class Egor{
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int a = sc.nextInt();
    int b = sc.nextInt();
    int n = sc.nextInt();
    if ((b*n)>=100){
      System.out.print(a*n + (b*n)/100 + " " + 0);
      }
     else{
      System.out.print(a*n + " " +  b*n);
      }
  }
 }
     
