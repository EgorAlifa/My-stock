import java.util.Scanner;

class Egor{
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
     double x = sc.nextDouble();
     double y = sc.nextDouble();
     if (x*2>=y){
      System.out.print("No");
      }
     else {
      System.out.print("Yes");
     }
    }
  }
