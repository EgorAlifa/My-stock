import java.util.Scanner;

class Egor{
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int a = sc.nextInt();
    int b = sc.nextInt();
    if (a%2!=0){
      a++;
      }
     while (a<=b){
      if (a%2==0){
        System.out.println(a);
        }
        a+=2;
    }
   }
 }
