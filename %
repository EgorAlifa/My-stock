import java.util.Scanner;

class Egor{
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int a = sc.nextInt();
    int b = sc.nextInt();
    int k = 0;
    while (a<=b){
      if ((a&3==0) && (a%5!=0)){
      k++;
      }
     a++;
     }
     System.out.print(k);
    }
  }
