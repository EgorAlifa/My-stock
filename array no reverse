import java.util.Scanner;

class Egor{
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    int[] a = new int[n];
    int k = -1;
    int s = 0;
    int l = -1;
    for (int i = 0; i<n; i++){
    k++;
    a[i]=sc.nextInt();
      if (k%2==0){
        s+=a[i];
      }
     }
     System.out.println(s);
     for (int i = 0; i<a.length;i++){
     l++;
     if (l%2==0){
      System.out.println(a[i]);
     }
    }
  }
}
     
