import java.util.Scanner;

class Egor{
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    int[] a = new int[n];
    int k = 0;
    for (int i = 0; i<n; i++){
      a[i]=sc.nextInt();
     }
     int s = sc.nextInt();
     int p = s;
     for (int i = 0; i<a.length;i++){
      k++;
      if (p>a[i]){
        System.out.print(k);
        break;
       }
      if (n==k){
        System.out.print(n+1);
      }
   }
 }
}
      
